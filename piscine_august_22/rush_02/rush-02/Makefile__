rush02: rush02.h main.o string_manipulation.o find_key.o fake_print_functions.o correctlenght.o
	gcc -o rush02 main.o string_manipulation.o find_key.o fake_print_functions.o correctlenght.o

main.o: main.o rush02.h string_manipulation.o find_key.o fake_print_functions.o correctlenght.o
	gcc -c main.c

string_manipulation.o: string_manipulation.o rush02.h main.o find_key.o fake_print_functions.o correctlenght.o
	gcc -c string_manipulation.c

find_key.o: find_key.o rush02.h main.o string_manipulation.o fake_print_functions.o correctlenght.o
	gcc -c find_key.c

fake_print_functions.o: fake_print_functions.o rush02.h main.o string_manipulation.o find_key.o correctlenght.o
	gcc -c fake_print_functions.c

correctlenght.o: correctlenght.o rush02.h main.o string_manipulation.o find_key.o fake_print_functions.o
	gcc -c correctlenght.c